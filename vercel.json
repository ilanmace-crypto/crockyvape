{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel-simple.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel-simple.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/server/vercel-simple.js"
    },
    {
      "src": "/health",
      "dest": "/server/vercel-simple.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel-simple.js"
    }
  ]
}
